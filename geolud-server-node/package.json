{
  "name": "geolud-server-node",
  "version": "1.6.1",
  "description": "geolud-server-node",
  "author": "fischermatte",
  "license": "WTFPL",
  "scripts": {
    "build": "tsc",
    "postbuild": "node zip-dist.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env PORT=8000 nodemon",
    "start:debug": "cross-env PORT=8000 nodemon --config nodemon-debug.json --verbose",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:cf": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cf:push:dev": "cf push",
    "cf:push:prod": "cf push -f manifest.prod.yml"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/graphql": "^6.2.4",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/platform-ws": "^6.3.1",
    "@nestjs/typeorm": "^6.1.2",
    "@nestjs/websockets": "^6.3.1",
    "apollo-server-express": "^2.6.3",
    "cfenv": "^1.2.2",
    "graphql": "^14.3.1",
    "graphql-tools": "^4.0.4",
    "mongodb": "^3.2.7",
    "nodemailer": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "typeorm": "^0.2.25",
    "typescript": "^3.5.2",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.3.1",
    "@types/express": "^4.17.0",
    "@types/graphql": "^14.2.0",
    "@types/jest": "^24.0.14",
    "@types/mongodb": "^3.1.28",
    "@types/node": "^12.0.8",
    "@types/nodemailer": "^6.2.0",
    "@types/supertest": "^2.0.7",
    "@types/ws": "^6.0.1",
    "archiver": "^3.0.0",
    "cross-env": "^5.2.0",
    "graphql-subscriptions": "^1.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.2.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.17.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^10",
    "npm": "^6"
  }
}
