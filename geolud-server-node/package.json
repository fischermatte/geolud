{
  "name": "geolud-server-node",
  "version": "1.6.0",
  "description": "geolud-server-node",
  "author": "fischermatte",
  "license": "WTFPL",
  "scripts": {
    "build": "tsc",
    "postbuild": "node zip-dist.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env PORT=8000 nodemon",
    "start:debug": "cross-env PORT=8000 nodemon --config nodemon-debug.json --verbose",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:cf": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "cf:push:dev": "cf push",
    "cf:push:prod": "cf push -f manifest.prod.yml"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.1",
    "@nestjs/core": "^5.7.3",
    "@nestjs/graphql": "^5.5.5",
    "@nestjs/typeorm": "^5.3.0",
    "@nestjs/websockets": "^5.7.3",
    "apollo-server-express": "^2.4.8",
    "cfenv": "^1.2.0",
    "graphql": "^14.1.1",
    "graphql-tools": "^4.0.4",
    "mongodb": "^3.1.10",
    "nodemailer": "^5.1.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.14",
    "typescript": "^3.0.1",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.7",
    "@types/jest": "^23.3.1",
    "@types/mongodb": "^3.1.10",
    "@types/node": "^10.7.1",
    "@types/nodemailer": "^4.6.6",
    "@types/supertest": "^2.0.5",
    "@types/ws": "^6.0.1",
    "archiver": "^2.1.1",
    "cross-env": "^5.2.0",
    "graphql-subscriptions": "^1.0.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^8.14.0",
    "npm": "^6.4.1"
  }
}
