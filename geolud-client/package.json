{
  "name": "geolud-client",
  "version": "1.6.1",
  "license": "WTFPL",
  "scripts": {
    "lint": "ng lint",
    "ng": "ng",
    "start": "npm-run-all -p start:server start:client",
    "start:client": "npm run serve:client",
    "start:server": "cd projects/geolud-client-server && npm run start",
    "serve:client": "ng serve",
    "serve:client:prod": "ng serve -c production",
    "serve:server": "cd projects/geolud-client-server && npm run serve",
    "build": "npm-run-all -p build:client build:server",
    "build:client": "ng build -c production --progress",
    "build:server": "cd projects/geolud-client-server && npm run build",
    "postbuild": "node zip-release-files.js",
    "release": "standard-version",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "e2e": "ng e2e",
    "format": "prettier --write \"**/*.ts\"",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
    "cf:push:dev": "cd dist && cf push",
    "cf:push:prod": "cd dist && cf push -f manifest.prod.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.0",
    "@angular/common": "9.0.0",
    "@angular/compiler": "9.0.0",
    "@angular/core": "9.0.0",
    "@angular/forms": "9.0.0",
    "@angular/platform-browser": "9.0.0",
    "@angular/platform-browser-dynamic": "9.0.0",
    "@angular/platform-server": "9.0.0",
    "@angular/router": "9.0.0",
    "@angular/service-worker": "9.0.0",
    "apollo-angular": "^1.8.0",
    "apollo-angular-link-http": "^1.9.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-ws": "^1.0.19",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "9.0.0",
    "@angular/language-service": "9.0.0",
    "@types/jasmine": "3.5.3",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "^13.7.0",
    "@types/uuid": "^3.4.7",
    "archiver": "^3.0.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~5.4.3",
    "standard-version": "^7.1.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5",
    "webdriver-manager": "^12.1.7"
  },
  "engines": {
    "node": "^10"
  }
}
