{
  "name": "geolud-client",
  "version": "1.6.1",
  "license": "WTFPL",
  "scripts": {
    "lint": "ng lint",
    "ng": "ng",
    "start": "npm-run-all -p start:server start:client",
    "start:client": "npm run serve:client",
    "start:server": "cd projects/geolud-client-server && npm run start",
    "serve:client": "ng serve",
    "serve:client:prod": "ng serve -c production",
    "serve:server": "cd projects/geolud-client-server && npm run serve",
    "build": "npm-run-all -p build:client build:server",
    "build:client": "ng build -c production --progress",
    "build:server": "cd projects/geolud-client-server && npm run build",
    "postbuild": "node zip-release-files.js",
    "release": "standard-version",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "e2e": "ng e2e",
    "format": "prettier --write \"**/*.ts\"",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
    "cf:push:dev": "cd dist && cf push",
    "cf:push:prod": "cd dist && cf push -f manifest.prod.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/platform-server": "8.2.14",
    "@angular/pwa": "^0.800.2",
    "@angular/router": "8.2.14",
    "@angular/service-worker": "8.2.14",
    "apollo-angular": "^1.6.0",
    "apollo-angular-link-http": "^1.8.0",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link": "^1.2.11",
    "apollo-link-ws": "^1.0.17",
    "core-js": "^2.6.4",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "rxjs": "~6.5.4",
    "uuid": "^3.3.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.24",
    "@types/uuid": "^3.4.4",
    "archiver": "^3.0.0",
    "codelyzer": "5.2.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "standard-version": "^7.1.0",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "tslib": "^1.10.0",
    "typescript": "~3.7.5",
    "webdriver-manager": "^12.1.1"
  },
  "engines": {
    "node": "^10"
  }
}
