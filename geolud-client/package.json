  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prod": "ng serve -c production",
    "start:prod:express": "node dist/server.js",
    "build": "ng build -c production",
    "postbuild": "node zip-dist.js",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "test:phantomjs": "ng test --browsers PhantomJS --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"src/**/*.ts\"",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
    "cf:push:dev": "cd dist && cf push",
    "cf:push:prod": "cd dist && cf push -f manifest.prod.yml"
  },
  "private": true,
