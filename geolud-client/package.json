{
  "name": "geolud-client",
  "version": "1.5.2",
  "license": "WTFPL",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p start:server start:client",
    "start:client": "npm run serve:client",
    "start:server": "cd projects/geolud-client-server && npm run start",
    "serve:client": "ng serve",
    "serve:client:prod": "ng serve -c production",
    "serve:server": "cd projects/geolud-client-server && npm run serve",
    "serve:server:prod": "cd projects/geolud-client-server && npm run serve:prod",
    "build": "npm-run-all -p build:client build:server",
    "build:client": "ng build -c production --progress",
    "build:server": "cd projects/geolud-client-server && npm run build",
    "postbuild": "node zip-release-files.js",
    "test": "ng test --browsers ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format": "prettier --write \"**/*.ts\"",
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
    "cf:push:dev": "cd dist && cf push",
    "cf:push:prod": "cd dist && cf push -f manifest.prod.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.4",
    "@angular/common": "7.2.4",
    "@angular/compiler": "7.2.4",
    "@angular/core": "7.2.4",
    "@angular/forms": "7.2.4",
    "@angular/http": "7.2.4",
    "@angular/platform-browser": "7.2.4",
    "@angular/platform-browser-dynamic": "7.2.4",
    "@angular/platform-server": "7.2.4",
    "@angular/pwa": "0.13.1",
    "@angular/router": "7.2.4",
    "@angular/service-worker": "7.2.4",
    "@ng-bootstrap/ng-bootstrap": "4.0.2",
    "bootstrap": "4.2.1",
    "core-js": "^2.6.4",
    "font-awesome": "4.7.0",
    "lato-font": "^3.0.0",
    "montserrat-font": "github:fischermatte/montserrat-font#v1.0.1",
    "ng-block-ui": "2.1.1",
    "ngx-toastr": "^9.1.2",
    "rxjs": "~6.4.0",
    "startbootstrap-freelancer": "github:fischermatte/startbootstrap-freelancer#4.1.1",
    "uuid": "^3.3.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.1",
    "@angular/cli": "^7.3.1",
    "@angular/compiler-cli": "7.2.4",
    "@angular/language-service": "7.2.4",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.24",
    "@types/uuid": "^3.4.4",
    "archiver": "^3.0.0",
    "codelyzer": "4.5.0",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.2",
    "tslint": "~5.12.1",
    "typescript": "~3.2.4",
    "webdriver-manager": "^12.1.1"
  },
  "engines": {
    "node": "^8.14.0",
    "npm": "^6.4.1"
  }
}
